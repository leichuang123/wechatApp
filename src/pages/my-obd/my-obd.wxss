page {
    background: #f1f1f1;
}
.car {
    width: 100%;
    height: 70px;
    background: white;
    margin-bottom: 8px;
    box-sizing: border-box;
    padding: 19px 25px 20px 14px;
    overflow: hidden;
}
.carimg {
    float: left;
    width: 30px;
    height: 30px;
    border-radius: 100%;
    overflow: hidden;
}
.carimg image {
    width: 30px;
    height: 30px;
    border-radius: 100%;
}
.carinformation {
    height: 40px;
    float: left;
    font-size: 14px;
    color: #101010;
    margin-left: 10px;
    overflow: hidden;
    margin-top: -2px;
}
.carstatus {
    float: right;
    font-size: 14px;
    color: #101010;
    height: 40px;
    line-height: 40px;
    overflow: hidden;
    margin-right: 10px;
}
.stautsicon {
    width: 8px;
    height: 8px;
    border-radius: 100%;
    background: #101010;
    float: left;
    margin-right: 10px;
    margin-top: 16px;
}
.online {
    background: #259b24;
}
.stop {
    background: #eb5c62;
}
.stautstext {
    float: left;
}
.carsee {
    float: right;
    height: 40px;
    text-align: center;
    line-height: 40px;
    border-left: 1px solid #bbbbbb;
    overflow: hidden;
}
.carsee image {
    width: 24px;
    height: 24px;
    margin-top: 8px;
    margin-left: 8px;
}
.addobd {
    position: fixed;
    width: 90%;
    height: 40px;
    bottom: 25px;
    left: 5%;
    line-height: 40px;
    border-radius: 5px;
    background-color: rgba(97, 184, 168, 1);
    color: rgba(255, 255, 255, 1);
    font-size: 14px;
    text-align: center;
    font-family: Roboto;
}
