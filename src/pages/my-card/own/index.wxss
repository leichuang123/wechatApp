.cards {
    background: #ffffff;
    height: auto;
    min-height: 100px;
    overflow: hidden;
}
.cards .member-face {
    width: 100%;
    border-radius: 0;
    margin-right: 0;
    padding: 6px;
    display: flex;
    align-items: center;
    overflow: hidden;
}
.member-face view {
    color: #101010;
    font-size: 14px;
    font-weight: 600;
}
.text-right {
    text-align: right;
}
.weui-cell {
    background: #fff;
}
.card-record__table {
    background-color: #fff;
    text-align: center;
    margin-bottom: 10px;
    font-size: 14px;
    padding-top: 10px;
    overflow: hidden;
}
.titles {
    display: flex;
    width: 100%;
    justify-content: space-between;
    overflow: hidden;
}
.titles {
    font-size: 12px;
}
.types {
    margin-left: 6px;
    font-weight: 600;
    columns: #101010;
    font-size: 14px;
}
.see {
    margin-right: 6px;
    color: #e60103;
}
.card-record__th {
    margin-bottom: 10px;
}

.table-header {
    padding-top: 10px;
    padding-bottom: 10px;
}

.table-row {
    margin-bottom: 5px;
}
.station-list {
    font-size: 12px;
}
.red {
    color: #e51c23;
}
