.box {
    background: #f1f1f1;
}
.type {
    width: 100%;
    height: 40px;
    background: #61b8a8;
    overflow: hidden;
    position: relative;
}
.da {
    position: absolute;
    left: 9px;
    top: 7px;
    font-size: 14px;
    color: #ffffff;
}
.date {
    position: absolute;
    right: 9px;
    top: 7px;
}
.date image {
    width: 24px;
    height: 24px;
}
.list {
    width: 96%;
    margin: 8px auto;
    height: 60px;
    border-radius: 5px;
    overflow: hidden;
    background: #ffffff;
    padding: 5px;
}
.time {
    width: 50px;
    height: 60px;
    float: left;
    color: #101010;
    font-size: 12px;
}
.s {
    overflow: hidden;
}
.start,
.start1,
.end,
.end1 {
    float: left;
}
.start {
    width: 10px;
    height: 10px;
    border-radius: 100%;
    background: green;
    margin-top: 4px;
}
.end {
    width: 10px;
    height: 10px;
    border-radius: 100%;
    background: red;
    margin-top: 4px;
}
.start1,
.end1 {
    margin-left: 3px;
    font-size: 12px;
}
.ins {
    float: left;
    width: 250px;
    color: #101010;
    font-size: 10px;
    margin-left: 6px;
}
.ins view {
    height: 30px;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.total {
    width: 50px;
    height: 60px;
    float: right;
    border-left: 1px solid #101010;
    text-align: center;
    font-size: #101010;
}
.total text {
    font-size: 12px;
}
