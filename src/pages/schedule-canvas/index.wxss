.box {
    background: #f1f1f1;
}
.canvas {
    width: 100%;
    height: 277px;
    background: white;
    position: relative;
}
ff-canvas {
    width: 100%;
    height: 100%;
}
.message {
    position: absolute;
    top: 18px;
    color: #101010;
    font-size: 14px;
    left: 48%;
    margin-left: -28px;
}
.all {
    width: 100px;
    height: 50px;
    text-align: center;
    font-size: 14px;
    color: #101010;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -50px;
    margin-top: -25px;
}
.nav {
    width: 100%;
    background: #ffffff;
    margin-top: 19px;
}
.links {
    background: #ffffff;
    padding: 12px 12px 10px 12px;
    border-bottom: 1px solid #f1f1f1;
    overflow: hidden;
}
.links view {
    font-size: 14px;
    color: #101010;
}
.icon {
    width: 15px;
    height: 15px;
    float: left;
    border-radius: 100%;
    margin-top: 4px;
    margin-right: 4px;
}
.s1 {
    background: #8a24a8;
}
.s2 {
    background: #ff9800;
}
.s3 {
    background: #726bda;
}
.s4 {
    background: #76d2f6;
}
.s5 {
    background: #259b24;
}
.s6 {
    background: #009688;
}
.s7 {
    background: #e51c23;
}
.linkstitle {
    float: left;
    height: 15px;
}
.linkstitle .bz {
    float: right;
    color: #101010;
    font-size: 12px;
    opacity: 0.5;
    margin-left: 5px;
    margin-top: 2px;
}
.linksins {
    float: right;
}
