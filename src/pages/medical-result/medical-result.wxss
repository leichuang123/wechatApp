page {
    background: #f4f4f4;
}
.part1 {
    width: 100%;
    height: 182px;
    background: #ffffff;
    overflow: hidden;
    padding: 10px 0;
    position: relative;
}
.speed {
    width: 60px;
    position: absolute;
    top: 49%;
    left: 50%;
    margin-left: -30px;
    font-size: 14px;
    color: #101010;
    text-align: center;
}
.oli {
    width: 60px;
    position: absolute;
    top: 55%;
    left: 17%;
    font-size: 14px;
    color: #101010;
    text-align: center;
}
.dis {
    width: 60px;
    position: absolute;
    top: 55%;
    right: 17%;
    font-size: 14px;
    color: #101010;
    text-align: center;
}
.part1 image {
    width: 100%;
    height: 100%;
}
.part2 {
    width: 100%;
    height: 120px;
    background: #ffffff;
    margin-top: 5px;
    overflow: hidden;
}
.pitem {
    float: left;
    width: 50%;
    height: 60px;
    box-sizing: border-box;
    padding: 5px;
    overflow: hidden;
}
.pitem view {
    font-size: 12px;
    color: #101010;
}
.pins {
    margin-top: 2px;
    overflow: hidden;
}
.pins .iconfont {
    width: 40px;
    height: 30px;
    text-align: center;
    line-height: 30px;
    float: left;
    color: #61b8a8;
    font-size: 28px;
}
.pins view {
    float: left;
    width: 30px;
    height: 30px;
    text-align: center;
    line-height: 30px;
    margin-left: 21px;
    font-size: 14px;
}
.pins .excel {
    width: 45px;
    float: right;
    margin-left: 0;
    color: #999999;
}
.border1 {
    border-right: 2px solid #f4f4f4;
    border-bottom: 2px solid #f4f4f4;
}
.border0 {
    border-bottom: 2px solid #f4f4f4;
}
.border2 {
    border-right: 2px solid #f4f4f4;
}
.part3 {
    margin-top: 12px;
    width: 100%;
    background: white;
    box-sizing: border-box;
    padding: 12px;
}
.result {
    display: flex;
    justify-content: space-between;
    font-size: 14px;
    color: #101010;
    margin-bottom: 7px;
}
.model {
    width: 100%;
    height: 100%;
    overflow: hidden;
    position: fixed;
    top: 0;
    left: 0;
    background-color: #dedede;
}
.title {
    background: #ffffff;
    width: 100%;
    text-align: center;
    color: #61b8a8;
    font-size: 22px;
    height: 50px;
    line-height: 50px;
}
.table {
    height: 380px;
    margin-top: 10px;
    background: #ffffff;
    box-sizing: border-box;
    padding: 10px 5px;
}
.table1 {
    border: 1px solid #101010;
    display: flex;
}
.table1 view {
    height: 40px;
    text-align: center;
    line-height: 40px;
    font-size: 16px;
}
.table2 {
    border: 1px solid #101010;
    border-top: none;
    border-bottom: none;
    overflow: hidden;
    display: flex;
}
.table2 view {
    font-size: 12px;
    height: 50px;
    border-bottom: 1px solid #101010;
}
.table2 .t1 {
    flex: 1;
    width: 70px;
    border-right: 1px solid #101010;
    text-align: center;
    height: 152px;
    line-height: 150px;
}
.table2 .t2 {
    flex: 1;
    width: 70px;
    border-right: 1px solid #101010;
    border-bottom: none;
    text-align: center;
    height: 150px;
    line-height: 50px;
}
.table2 .t3 {
    flex: 1;
    width: 70px;
    border-right: 1px solid #101010;
    border-bottom: none;
    text-align: center;
    height: 150px;
    line-height: 50px;
}
.table2 .t4 {
    width: 150px;
    flex: 2;
    height: 150px;
    border-bottom: none;
    width: 150px;
}
.red {
    color: red;
}
.close {
    width: 90%;
    height: 40px;
    text-align: center;
    line-height: 40px;
    color: #ffffff;
    background-color: #61b8a8;
    border-radius: 5px;
    position: absolute;
    left: 5%;
    bottom: 10px;
}
