<view>
    <view class="part1">
        <view class="speed">{{result.speed}}</view>
        <view class="oli">{{result.fuel}}</view>
        <view class="dis">{{result.distance}}</view>
        <image src="../../assets/images/icons/carer.png" />
    </view>
    <view class="part2">
        <view class="pitem border1">
            <view>电瓶电压</view>
            <view class="pins">
                <image src="../../assets/images/icons/dianya.png" />
                <view>{{result.vol}}v</view>
                <view class="excel" bindtap="showexcel">对照表</view>
            </view>
        </view>
        <view class="pitem border0">
            <view>发动机转速</view>
            <view class="pins">
                <image src="../../assets/images/icons/xinxinicon.png" />
                <view>{{result.rpm}}r/min</view>
            </view>
        </view>
        <view class="pitem border2">
            <view>冷却液温度</view>
            <view class="pins">
                <image src="../../assets/images/icons/temperature.png" />
                <view>{{result.coolant_temp}}*C</view>
            </view>
        </view>
        <view class="pitem">
            <view>空燃比系数</view>
            <view class="pins">
                <image src="../../assets/images/icons/yibiaopans.png" />
                <view>{{result.air_fuel}}km/h</view>
            </view>
        </view>
    </view>
    <view class="part3">
        <view class="result">
            <view>方向</view>
            <view>{{result.steering_angle}}</view>
        </view>
        <view class="result">
            <view>GPS卫星个数</view>
            <view>{{result.satellite}}</view>
        </view>
        <view class="result">
            <view>GMS信号质量</view>
            <view>{{result.singal}}</view>
        </view>
        <view class="result">
            <view>负荷计算值</view>
            <view>{{result.load}}</view>
        </view>
        <view class="result">
            <view>OBD车速</view>
            <view>{{result.speed}}</view>
        </view>
        <view class="result">
            <view>点火提前角</view>
            <view>{{result.ignition_angle}}</view>
        </view>
        <view class="result">
            <view>进气歧管绝对压力</view>
            <view>{{result.intake_pressure}}</view>
        </view>
        <!-- <view class="result">
            <view>控制模块电压</view>
            <view>{{result.load}}</view>
        </view> -->
        <view class="result">
            <view>进气温度</view>
            <view>{{result.intake_temp}}</view>
        </view>
        <view class="result">
            <view>空气流量</view>
            <view>{{result.air_flow}}</view>
        </view>
        <view class="result">
            <view>节气门相对位置</view>
            <view>{{result.relative_throttle}}</view>
        </view>
        <view class="result">
            <view>节气门绝对位置</view>
            <view>{{result.throttle}}</view>
        </view>
        <view class="result">
            <view>长期燃油修正</view>
            <view>{{result.fuel_trim}}</view>
        </view>
        <view class="result">
            <view>燃油压力</view>
            <view>{{result.fuel_pressure}}</view>
        </view>
        <view class="result">
            <view>油耗</view>
            <view>{{result.fuel}}</view>
        </view>
    </view>
    <block wx:if="{{excel}}">
        <view class="model">
            <image src="../../assets/images/icons/excel.png" />
            <view class="close" bindtap="hidexcel">关闭</view>
        </view>
    </block>
</view>