.box {
    background: #f1f1f1;
}
.type {
    width: 100%;
    height: 40px;
    background: #61b8a8;
    overflow: hidden;
}
.typecheck {
    width: 213px;
    height: 25px;
    border: 1px solid white;
    display: flex;
    border-radius: 25px;
    margin: 6px auto;
    overflow: hidden;
}
.typecheck view {
    flex: 1;
    height: 25px;
    text-align: center;
    line-height: 25px;
    color: white;
    font-size: 14px;
}
.typecheck view:nth-child(3) {
    border-top-right-radius: 25px;
    border-bottom-right-radius: 25px;
}
.typecheck .active {
    background: #ffffff;
    color: #61b8a8;
}
.date {
    position: absolute;
    right: 9px;
    top: 7px;
}
.date .iconfont {
    color: #ffffff;
    font-size: 24px;
}
.canvas {
    width: 100%;
    height: 300px;
    background: white;
    overflow: hidden;
    position: relative;
}
ff-canvas {
    width: 100%;
    height: 100%;
}
.canvas text {
    color: #101010;
    font-size: 14px;
    position: absolute;
    top: 2px;
    left: 12px;
}
.nav {
    width: 100%;
    background: #ffffff;
    margin-top: 19px;
}
.links {
    background: #ffffff;
    padding: 12px;
    border-bottom: 1px solid #f1f1f1;
    overflow: hidden;
}
.links view {
    font-size: 14px;
    color: #101010;
}
.linkstitle {
    float: left;
}
.linksicon {
    float: right;
}
.links .ft {
    font-size: 16px;
    margin-top: 2px;
}
.linksins {
    float: right;
}
