.goodList {
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
    width: 100%;
    box-sizing: border-box;
    padding: 6px;
}
.goods {
    background-color: #ffffff;
    width: 49%;
    height: 245px;
    border-radius: 5px;
    text-align: center;
    border: 1px solid rgba(255, 255, 255, 0);
    margin-bottom: 6px;
    overflow: hidden;
    box-sizing: border-box;
    padding: 5px;
}
.goodImg image {
    width: 98%;
    height: 150px;
}
.goodIns {
    width: 100%;
    font-size: 14px;
    color: #101010;
    height: 40px;
    overflow: hidden;
    text-overflow: ellipsis;
    text-align: left;
}
.goodOther {
    margin-top: 8px;
    width: 100%;
    display: flex;
    justify-content: space-between;
}
.recommendPrice {
    font-size: 14px;
    color: #ff0000;
}
.buy {
    width: 40px;
    height: 20px;
    line-height: 20px;
    border-radius: 2px;
    background-color: rgba(230, 1, 3, 1);
    color: rgba(255, 255, 255, 1);
    font-size: 12px;
    text-align: center;
    font-family: Roboto;
}
