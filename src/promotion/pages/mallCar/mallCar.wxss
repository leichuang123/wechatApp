.data {
    padding-bottom: 60px;
}
.length {
    color: #373c45;
    font-size: 14px;
    width: 95%;
    margin: 0 auto;
    padding-top: 13px;
}
.empty {
    font-size: 14px;
    text-align: center;
    padding: 20px 0;
    color: #373c45;
}
.box {
    width: 95%;
    box-sizing: border-box;
    margin: 13px auto;
    background-color: #ffffff;
    border-radius: 10px;
    padding: 15px;
    display: flex;
    justify-content: flex-start;
    position: relative;
}
.boxed {
    width: 95%;
    box-sizing: border-box;
    margin: 13px auto;
    background-color: #ffffff;
    border-radius: 10px;
    padding: 15px;
}
.noMargin {
    padding: 0;
    width: 100%;
    margin: 10px auto;
}
.boxInfo {
    margin-left: 10px;
    width: 100%;
}
.boxTitle {
    color: #373c45;
    font-size: 14px;
    height: 44px;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.boxImg {
    margin-left: 10px;
}
.big {
    width: 76px;
    height: 76px;
}
.price {
    display: flex;
    justify-content: space-between;
    margin-top: 10px;
    color: #373c45;
    font-size: 14px;
}
.salePrice {
    color: rgba(230, 1, 3, 1);
}
.board {
    display: flex;
    justify-content: flex-start;
    width: 97px;
    height: 21px;
    line-height: 20px;
    border-radius: 4px 4px 4px 4px;
    background-color: rgba(255, 255, 255, 1);
    text-align: center;
    border: 1px solid rgba(220, 223, 230, 1);
}
.value {
    width: 100px;
    height: 21px;
    line-height: 20px;
    text-align: center;
}
.action {
    width: 90px;
    height: 21px;
    line-height: 20px;
    border-radius: 4px 0px 0px 4px;
    background-color: rgba(245, 247, 250, 1);
    text-align: center;
}
.rechange {
    width: 50px;
    height: 21px;
    line-height: 21px;
    border-radius: 30px;
    background-color: rgba(255, 255, 255, 0);
    color: rgba(230, 1, 3, 1);
    font-size: 10px;
    text-align: center;
    font-family: Roboto;
    border: 1px solid rgba(230, 1, 3, 1);
}
.isMax {
    color: #929292;
    text-align: right;
    font-size: 12px;
    margin-top: 5px;
}
.otherPrice {
    color: #929292;
    font-size: 12px;
}
.dieTop {
    display: flex;
    justify-content: space-between;
    font-size: 14px;
    color: #373c45;
    margin-bottom: 20px;
}
.die {
    color: rgba(230, 1, 3, 1);
}
.dieIcon {
    width: 40px;
    height: 17px;
    text-align: center;
    line-height: 17px;
    border-radius: 30px;
    background-color: rgba(182, 182, 182, 1);
    color: rgba(255, 255, 255, 1);
    font-size: 12px;
    text-align: center;
    font-family: Roboto;
    border: 1px solid rgba(255, 255, 255, 0);
}
.actionBar {
    width: 100%;
    background-color: #ffffff;
    height: 50px;
    line-height: 30px;
    display: flex;
    justify-content: space-between;
    box-sizing: border-box;
    padding: 10px 20px;
    position: fixed;
    bottom: 0;
    left: 0;
    border-top: 1px solid #f1f1f1;
    overflow: hidden;
}
.flexs {
    display: flex;
    justify-content: flex-start;
    align-items: center;
}
.radioAll {
    font-size: 12px;
    margin-top: -50px;
}
.delSome {
    font-size: 12px;
    color: rgba(230, 1, 3, 1);
    margin-left: 10px;
    margin-top: 2px;
}
.actionInfo {
    margin-right: 15px;
    font-size: 12px;
    color: #373c45;
}
.allMoney {
    color: rgba(230, 1, 3, 1);
    font-size: 13px;
}
.pay {
    width: 100px;
    height: 30px;
    line-height: 30px;
    border-radius: 30px;
    background-color: rgba(230, 1, 3, 1);
    color: rgba(255, 255, 255, 1);
    font-size: 14px;
    text-align: center;
    font-family: Roboto;
    border: 1px solid rgba(230, 1, 3, 1);
}
