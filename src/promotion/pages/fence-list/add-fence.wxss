page {
    overflow: hidden;
}
#myMap {
    width: 100%;
}
.slider {
    width: 100%;
    background: #ffffff;
    overflow: hidden;
    padding-bottom: 8px;
}
.adress {
    color: #101010;
    font-size: 14px;
    box-sizing: border-box;
    padding: 10px;
    display: flex;
    justify-content: left;
    border-bottom: 1px solid #bbbbbb;
}
.adress view:nth-child(1) {
    width: 50px;
}
.sliders {
    margin-top: 13px;
}
.sliders {
    color: #101010;
    font-size: 14px;
    box-sizing: border-box;
    padding: 5px 10px;
    display: flex;
    justify-content: left;
    position: relative;
}
.sliders view:nth-child(1) {
    width: 50px;
}
.sliders .s {
    width: 242px;
    margin-top: -2px;
}
.fw {
    color: #101010;
    opacity: 0.6;
}
.sliderscale {
    position: absolute;
    top: -12px;
    left: 50%;
    color: #101010;
    opacity: 0.6;
}
.ok {
    width: 80px;
    height: 30px;
    border-radius: 5px;
    line-height: 30px;
    font-size: 14px;
    text-align: center;
    background: #009688;
    color: #ffffff;
    margin: 0 auto;
}
.circle {
    width: 300px;
    height: 300px;
    border-radius: 100%;
    background-color: rgba(0, 0, 0, 0.4);
    position: fixed;
    top: 152px;
    left: 50%;
    margin-left: -150px;
    z-index: 999;
}
.model {
    width: 100%;
    height: 100%;
    background-color: #00000080;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 9999;
}
.modeler {
    width: 90%;
    border-radius: 3px;
    border: 1px solid rgba(187, 187, 187, 1);
    background-color: white;
    top: 160px;
    left: 5%;
    position: absolute;
    box-sizing: border-box;
    padding: 10px;
    overflow: hidden;
}
.modeler input {
    margin-top: 5px;
    border-bottom: 1px solid #bbbbbb;
    color: #888888;
    font-size: 14px;
}
.modeler text {
    color: #101010;
    font-size: 14px;
    margin-top: 8px;
}
.modeler label {
    color: #101010;
    font-size: 14px;
}
.action {
    display: flex;
    height: 37px;
    margin-top: 13px;
}
.action button {
    width: 46%;
    height: 37px;
    text-align: center;
    line-height: 37px;
    color: #E60103;
    font-size: 14px;
    background: #ffffff;
    border: none;
    outline: none;
}
.action button:nth-child(2) {
    background: #E60103;
    color: #ffffff;
}
