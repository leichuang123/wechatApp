.box {
    background: #f1f1f1;
}
.rate {
    width: 100%;
    height: 120px;
    overflow: hidden;
    text-align: center;
    background: #f1f1f1;
}
.pingji {
    margin-top: 21px;
    font-size: 14px;
    color: #101010;
}
.xingji {
    margin-top: 7px;
}
.xingji text {
    margin-right: 5px;
}
.pingjia {
    margin-top: 13px;
    font-size: 12px;
    color: #101010;
}
.list {
    width: 100%;
    margin: 8px auto;
    overflow: hidden;
    background: #ffffff;
    box-sizing: border-box;
    padding: 10px;
    display: flex;
    justify-content: space-between;
}
.time {
    height: 60px;
    color: #101010;
    font-size: 12px;
}
.s {
    overflow: hidden;
}
.start,
.start1,
.end,
.end1 {
    float: left;
}
.start {
    width: 10px;
    height: 10px;
    border-radius: 100%;
    background: green;
    margin-top: 4px;
}
.end {
    width: 10px;
    height: 10px;
    border-radius: 100%;
    background: red;
    margin-top: 4px;
}
.start1,
.end1 {
    margin-left: 3px;
}
.ins {
    width: 280px;
    color: #101010;
    font-size: 12px;
    margin-left: 6px;
}
.nav {
    width: 100%;
    background: #ffffff;
    margin-top: 19px;
}
.links {
    background: #ffffff;
    padding: 12px;
    border-bottom: 1px solid #f1f1f1;
    overflow: hidden;
}
.links view {
    font-size: 14px;
    color: #101010;
}
.linkstitle {
    float: left;
}
.linksicon {
    float: right;
}
.linksins {
    height: 24px;
    line-height: 24px;
    float: right;
}
.links .ft {
    font-size: 16px;
    margin-top: 2px;
}
