#myMap {
    width: 100%;
}
.sz {
    position: fixed;
    z-index: 99999;
    width: 32px;
    height: 32px;
    border-radius: 5px;
    text-align: center;
    top: 35px;
    right: 13px;
    background: #61b8a8;
}
.s {
    width: 32px;
    height: 32px;
    text-align: center;
    line-height: 32px;
    border-radius: 5px;
}

.select {
    position: fixed;
    z-index: 99999;
    width: 100%;
    height: 191px;
    bottom: 0;
    left: 0;
    overflow: hidden;
    background: #ffffff;
    text-align: center;
}
.title {
    width: 100%;
    height: 40px;
    line-height: 40px;
    text-align: center;
    font-size: 14px;
    color: #101010;
    border-bottom: 1px solid #f1f1f1;
}
.items {
    width: 100%;
    height: 50px;
    line-height: 50px;
    text-align: center;
    color: #101010;
    font-size: 14px;
}
.ac {
    color: #61b8a8;
    background: #f1f1f1;
}
